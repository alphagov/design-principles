#wrapper article .examples .design .example {
  clear: both;
  float: left;
  display: inline;
  width: 66.66666666%;
}

#wrapper article .examples .design .example img {
  position: relative;
  top: 5px;
  top: 0.5rem;
  margin: 0 0 13px 0.74074074em;
  margin: 0 0 1.3rem 0.74074074em;
  width: 97%;
