}

#wrapper article .examples .design .caption {
  float: right;
  display: inline;
  width: 33.33333333%;
}

#wrapper article .examples .design .caption h2,
#wrapper article .examples .design .caption p {
  padding: 0 13px 13px;
  padding: 0 1.3rem 1.3rem;
}

@media (max-width: 640px) {
  #wrapper article .examples .design .example,
  #wrapper article .examples .design .caption {
    clear: both;
    width: auto;
    padding: 0 13px;
    padding: 0 1.3rem;
  }

  #wrapper article .examples .design .example img {
    width: 100%;
    margin-left: 0;
  }
  
  #wrapper article .examples .design .caption h2,
  #wrapper article .examples .design .caption p {
