/*main.css*/

[if IE7] * {
  position: static;
}

html {
  font-size: 62.5%;
}

@media (max-width: 900px) {
  html {
    font-size: 9px;
  }
}

@media (min-width: 1100px) {
  html {
    font-size: 11px;
  }
}

@media (min-width: 1200px) {
  html {
    font-size: 12px;
  }
}

@media (min-width: 1300px) {
  html {
    font-size: 13px;
  }
}

@media (min-width: 1400px) {
  html {
    font-size: 14px;
  }
}

@media (min-width: 1500px) {
  html {
    font-size: 15px;
  }
}

.visuallyhidden,
.figure-mask {
    height: 1px;
    width: 1px;
    margin:-1px;
    overflow: hidden;
    padding: 0; 
    position: absolute;    
}

#wrapper {
  position: relative;
  color: #000;
  background: #fff;
  font-family: "Gill Sans W01 Book", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
  -webkit-font-smoothing: antialiased;
  font-size: 18px;
  font-size: 1.8rem;
  line-height: 1.4444444444;
  font-weight: 300;
  padding: 26px 52px 0 104px;
  padding: 2.6rem 5.2rem 0 10.4rem;
  margin: 0;
}

@media (max-width: 640px) {
  #wrapper {
    padding: 0 26px;
    padding: 0 2.6rem;
  }
}

#wrapper * {
  margin: 0;
  padding: 0;
  font-family: inherit;
  font-size: inherit;
  font-weight: inherit;
  color: inherit;
}

#wrapper ol, 
#wrapper ol > li {
  list-style: none;
}

.lte-ie8 #wrapper ol li li {
  list-style-position: inside;
  list-style-type: bullet;
}

#wrapper header, #wrapper nav,
#wrapper .principles,
#wrapper .additional,
#wrapper .feedback{
  position: relative;
  margin: 0 auto;
  max-width: 700px;
  max-width: 70rem;
}

#wrapper a {
  color: black;
  text-decoration: none;
  border-bottom: 1px solid #2E8ACA;
}

#wrapper a:hover,
#wrapper a:focus,
#wrapper nav a:hover .caption,
#wrapper nav a:focus .caption {
  border-color: #ddd;
}

#wrapper a:active {
  color: #2E8ACA;
}

#wrapper nav a {
  border-bottom: none;
}

#wrapper nav a .icon,
#wrapper .checklist .icon {
  position: relative;
  float: left;
  clear: both;
  text-align: right;
  margin-left: -2.1666666em;
  margin-left: -3.9rem;
  width: 1.4444444em;
  width: 2.6rem;
  color: #aaa;
}

@media (max-width: 640px) {
  
  #wrapper nav a .icon {
    margin-left: 0;
    text-align: left;
  }

}

#wrapper nav a .caption {
  border-bottom: 1px solid #ddd;
}

#wrapper h1 {
  font-size: 45px;
  font-size: 4.5rem;
  line-height: 1.1555555556;
  font-weight: 300;
  padding: 6px 0 8px;
  padding: 1.8rem 0 0.8rem;
}

@media (max-width: 640px) {
  #wrapper h1 {
    font-size: 26px;
    font-size: 2.6rem;
  }

  #wrapper > header h1 {
    margin-top: 26px;
    margin-top: 2.6rem;
  }
}

#wrapper h1 strong,
#wrapper article h1 {
  font-family: "Gill Sans W01 Medium", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
  display: block;
  font-weight: 500;
  letter-spacing: 0.01em;
}

#wrapper header, 
#wrapper article {
  position: relative;
  clear: both;
  padding-bottom: 26px;
  padding-bottom: 2.6rem;
  padding-top: 78px;
  padding-top: 7.8rem;
  min-height: 1px;
}

#wrapper header p {
  max-width: 85%;
  padding: 23px 0 3px;
  padding: 2.3rem 0 0.3rem;
}

#wrapper .principles {
  padding-bottom: 78px;
  padding-bottom: 7.8rem;  
}

#wrapper .principles:after {
  content: '.';
  clear: both;
  visibility: hidden;
}

#wrapper nav {
  padding-top: 26px;
  padding-top: 2.6rem;
}

@media (max-width: 640px) {
  #wrapper header, 
  #wrapper article {
    padding-top: 26px;
    padding-top: 2.6rem;
  }
  #wrapper nav {
    padding-top: 0;
  }
}

#wrapper article h1 {
  color: #dd322d;
}

#wrapper article h1:before {
  position: relative;
  float: left;
  clear: both;
  text-align: right;
  margin-left: -78px;
  margin-left: -7.8rem;
  width: 52px;
  width: 5.2rem;
  font-weight: 300;
  letter-spacing: -0.05em;
}

@media (max-width: 640px) {
  #wrapper article h1:before {
    margin-left: 0;
    width: auto;
    padding-right: 0.5em;
  }
}

#first:before {
  content: "1";
}

#second:before {
  content: "2";
}

#third:before {
  content: "3";
}

#fourth:before {
  content: "4";
}

#fifth:before {
  content: "5";
}

#sixth:before {
  content: "6";
}

#seventh:before {
  content: "7";
}

#eighth:before {
  content: "8";
}

#ninth:before {
  content: "9";
}

#tenth:before {
  content: "10";
}

#wrapper .outline p {
  font-size: 32px;
  font-size: 3.2rem;
  line-height: 1.21875;
  font-weight: 300;
  padding: 14px 0 12px;
  padding: 1.4rem 0 1.2rem;
}

#wrapper .outline p img {
  float: right;
  clear: both;
  width:auto;
  height: auto;
  max-width: 100%;
}

@media (max-width: 640px) {
  #wrapper .outline p {
    font-size: 21px;
    font-size: 2.1rem;
    padding: 8px 0 2px;
    padding: 0.8rem 0 0.2rem;
  }

  #wrapper .outline p img {
    display: none;
  }
}

#wrapper p.outline img.col-1of2 {
  width: 50%;
}

#wrapper p.outline img.col-2of3 {
  width: 66.6666666666%;
}

#wrapper p.outline img.col-1of2-5 {
  width: 40%;
}

#wrapper p.outline img.col-1of3 {
  width: 33.3333333333%;
}

#wrapper article h2,
#wrapper header h2 {
  font-family: "Gill Sans W01 Medium", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
  font-size:1em;
  font-size: 1.3rem;
  line-height: 1.1538461538;
  font-weight: 500;
  letter-spacing: 0.2em;
  padding: 16px 0 8px;
  padding: 1.6rem 0 0.8rem;
}

#wrapper header h2 {
  padding-bottom:0;
}

#wrapper article #first + h2 {
  position: relative;
  top: -13px;
  top: -1.3rem;
  color: #dd322d;
}

#wrapper article h3 {
  color: #DD322D;
  letter-spacing: 0;
  padding: 10px 0 3px;
  padding: 1rem 0 0.3rem;
}

#wrapper article ul {
  margin-bottom: 13px;
  margin-bottom: 1.3rem;
}

#wrapper article .content p {
  letter-spacing: 0;
  padding: 10px 0 3px;
  padding: 1rem 0 0.3rem;
}

#wrapper article .content ul,
#wrapper article .content ol {
  padding:1.6rem 0 0.8rem;
}

#wrapper article section {
  position: relative;
  padding-top: 13px;
  padding-top: 1.3rem;
}

#wrapper article .examples {
  clear: both;
  overflow: visible;
}

#wrapper article .examples .content {
  position: relative;
  clear: both;
  width: auto;
  margin: 0 -13px;
  margin: 0 -1.3rem;
  overflow: hidden;
  display: block;
}

#wrapper article .examples a:hover {
  border-color: #fff;
}

#wrapper article .examples .example,
#wrapper article .example a {
  border: none;
}

#wrapper article header {
  padding: 0;
}

#wrapper article .examples .process,
#wrapper article .examples .writing,
#wrapper article .examples .code,
#wrapper article .examples .design,
#wrapper article .examples .comparison,
#wrapper article .examples .table-view {
  position: relative;
  overflow: hidden;
  margin-top: 1px;
  padding: 0;
  display: block;
  background: #ddd;
}

#wrapper article .examples header {
  padding: 0 13px;
  padding: 0 1.3rem;
}

#wrapper article .worked-example {
  padding-top:0;
}

#wrapper article .examples h1,
#wrapper article .worked-example h1 {
  border: none;
  color: black;
  font-family: Helmet, Freesans, sans-serif;
  font-size: 18px;
  font-size: 1.8rem;
  font-weight: bold;
  padding-top: 9px;
  padding-top: 0.9rem;
  max-width: 66.666%;
}

#wrapper article .examples h3 {
  clear: both;
  padding: 26px 13px 13px;
  padding: 2.6rem 1.3rem 1.3rem;
}

#wrapper article .examples .caption {
  font-family: Helmet, Freesans, sans-serif;
  font-weight: 400;
  font-size: 15px;
  font-size: 1.5rem;
}

#wrapper article .examples .highlight{
  font-weight: bold;
  color: #dd322d;
  background-color: #f8f8f8;
}

.header-context,
#footer .beta-notice{
  display:none;
}

#wrapper .beta-notice {
  color: #000;
  float: left;
  
  font-family: "Gill Sans W01 Medium", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
  text-transform: uppercase;
  font-size: 14px;
  font-size: 1.4rem;
  line-height: 1.1538461538;
  font-weight: 500;
  letter-spacing: 0.2em;
  
  display:block;
  position:absolute;
  z-index: 100;
  top:42px;
  left: 50%;
  margin-left: -478px;
  margin-left: -32.3rem;
}

#wrapper.performanceframework .beta-notice span {
  padding-left:0;
}

@media (max-width: 640px) {
  #wrapper .beta-notice {
    top: 0;
    left: 0;
    margin-left: 26px;
    margin-left: 2.6rem;
  }
}

#wrapper .beta-notice a{
  padding: 8px 13px;
  padding: 0.8rem 1.3rem;
  background-color:#d43783;
  border: none;
  float:left;
  color:#fff;
  text-decoration: none;
}
#wrapper .beta-notice span{
  padding: 8px 13px;
  padding: 0.8rem 1.3rem;
  float:left;
}
@media (max-width: 640px) {
  #wrapper .beta-notice span{
    display: block;
    padding-left: 0;
    clear:both;
  }
}

#wrapper .alpha-feedback,
#wrapper .beta-feedback {
  position: relative;
  clear:both;
  padding: 0 26px 26px;
  padding: 0 2.6rem 2.6rem;
}

#wrapper .alpha-feedback {
  background-color:#d43783;
  color:#fff;
}

#wrapper .beta-feedback {
  background-color:#F17921;
  color:#000;
}

@media (max-width: 640px) {
  #wrapper .alpha-feedback{
    padding: 0 13px 13px;
    padding: 0 1.3rem 1.3rem;
  }
}

#wrapper .alpha-feedback h1,
#wrapper .beta-feedback h1 {
  font-size: 32px;
  font-size: 3.2rem;
}

#wrapper .alpha-feedback a,
#wrapper .beta-feedback a {   
  border-color: white;
  font-family: "Gill Sans W01 Medium", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
}

#wrapper .alpha-feedback a {
  color: white;
}

#wrapper .beta-feedback a {
  border-bottom:1px solid #000;
}

#wrapper .alpha-feedback header,
#wrapper .beta-feedback header {
  padding: 0;
  margin: 0;
}

#wrapper article .example {
  background: transparent;
}

#wrapper article .examples p.status {
  position: absolute;
  right: 0;
  top: 8px;
  top: 0.8rem;
}

@media (max-width: 640px) {
  #wrapper article .examples p.status {
    font-family: "Gill Sans W01 Medium", "Gill Sans MT Pro", "Gill Sans", GillSans, sans-serif;
  }
}

#wrapper article .examples .code .example {
  margin: 0 13px;
  margin: 0 1.3rem;
  padding: 13px;
  padding: 1.3rem;
  background: white;
  border: 1px solid #ccc;
  color: black;
  font-family: monospace;
  line-height: 1.3;
  font-size: 14px;
  font-size: 1.4rem;
}

#wrapper article .examples .code .example * {
